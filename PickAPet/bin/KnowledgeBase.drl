   rule "Maltańczyk"
 	salience 150
 	when
 		Property(type=="age" && value=="powyżej 60")
 		Property(type=="childSubstitute" && value=="tak")
 		Property(type=="dogPossible" && value=="tak")
 		Property(type=="otherAnimals" && value=="tak")
 		(or (Property(type=="otherAnimalsFriendliness" && value=="psy"))
 		    (Property(type=="otherAnimalsFriendliness" && value=="koty"))
 		    (Property(type=="otherAnimalsFriendliness" && value=="psy i koty"))
 		 )
 		Property(type=="fur" && value=="długa")
 	then            
        MainWindow.displayResults("Rekomendowanym dla ciebie zwierzęciem jest Maltańczyk.", null);
 end
 
